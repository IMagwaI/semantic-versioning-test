{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "commit":"git-cz",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "package-test-semantic",
  "author": "",
  "module": "dist/package-test-semantic.esm.js",
  "size-limit": [
    {
      "path": "dist/package-test-semantic.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/package-test-semantic.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@size-limit/preset-small-lib": "^9.0.0",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/addons": "^7.5.0",
    "@storybook/react": "^7.5.0",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "babel-loader": "^9.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "semantic-release": "^22.0.5",
    "size-limit": "^9.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "4.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IMagwaI/semantic-versioning-test.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
